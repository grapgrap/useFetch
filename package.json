{
  "name": "@grapgrap/use-fetch",
  "version": "0.3.6",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/grapgrap/useFetch.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "set-husky": "husky install",
    "sdk-update": "yarn dlx @yarnpkg/sdks vscode",
    "postinstall": "yarn set-husky && yarn sdk-update",
    "test": "jest --config jest.config.js",
    "clean:cache": "rm -rf tsconfig.tsbuildinfo && rm -rf .rollup.cache",
    "clean:dist": "rm -rf dist",
    "build": "yarn clean:dist && rollup --config rollup.config.js",
    "type": "tsc --project tsconfig.json --noEmit",
    "prepublishonly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "yarn prepublishonly && yarn npm publish --access public --tolerate-republish && yarn postpublish"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "fast-json-stable-stringify": "^2.1.0",
    "history": "^5.2.0",
    "swr": "^1.1.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.3",
    "@types/fast-json-stable-stringify": "^2.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.17",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "core-js": "^3.20.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "msw": "^0.36.3",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.62.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
