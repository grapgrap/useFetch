{
  "name": "@grapgrap/use-fetch",
  "version": "0.3.8",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/grapgrap/useFetch.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "set-husky": "husky install",
    "sdk-update": "yarn dlx @yarnpkg/sdks vscode",
    "postinstall": "yarn set-husky && yarn sdk-update",
    "test": "jest --config jest.config.js",
    "clean:cache": "rm -rf tsconfig.tsbuildinfo && rm -rf .rollup.cache",
    "clean:dist": "rm -rf dist",
    "build": "yarn clean:dist && rollup --config rollup.config.js",
    "type": "tsc --project tsconfig.json --noEmit",
    "prepublishonly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "yarn prepublishonly && yarn npm publish --access public --tolerate-republish && yarn postpublish"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "fast-json-stable-stringify": "^2.1.0",
    "history": "^5.2.0",
    "swr": "^1.1.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/fast-json-stable-stringify": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "core-js": "^3.18.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "msw": "^0.35.0",
    "parcel": "^2.3.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
